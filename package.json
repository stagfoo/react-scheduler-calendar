  "scripts": {
    "test": "echo 'No test' #cross-env CI=1 react-scripts-ts test --env=jsdom",
    "test:watch": "react-scripts-ts test --env=jsdom",
    "generate:declaration": "tsc --declaration --allowJs --emitDeclarationOnly --outDir ./dist",
    "build": "yarn generate:declaration && webpack --config ./config/webpack.prod.conf.js",
    "start:dev": "webpack-dev-server --config ./config/webpack.dev.conf.js",
    "prepublishOnly": "yarn build",
    "deploy:demo": "cd example && yarn deploy",

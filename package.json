{
  "name": "react-scheduler-calendar",
  "version": "0.3.0",
  "description": "React component for scheduling/planing in calendar",
  "author": "NGC-AB",
  "license": "MIT",
  "repository": "NGC-AB/react-scheduler-calendar",
  "types": "dist/index.d.ts",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "engines": {
    "node": ">=8",
    "npm": ">=5"
  },
  "scripts": {
    "test": "echo 'No test' #cross-env CI=1 react-scripts-ts test --env=jsdom",
    "test:watch": "react-scripts-ts test --env=jsdom",
    "generate:declaration": "tsc --declaration --allowJs --emitDeclarationOnly --outDir ./dist",
    "build": "yarn generate:declaration && webpack --config ./config/webpack.prod.conf.js",
    "start:dev": "webpack-dev-server --config ./config/webpack.dev.conf.js",
    "prepare": "yarn run build",
    "deploy:demo": "cd example && yarn deploy",
    "release": "np --no-publish"
  },
  "dependencies": {
    "moment": "^2.26.0",
    "react-dnd": "^11.1.3",
    "react-dnd-html5-backend": "^11.1.3",
    "react-dnd-touch-backend": "^11.1.3",
    "rrule": "^2.6.4"
  },
  "peerDependencies": {
    "antd": "^4.0.3",
    "moment": "^2.26.0",
    "react": "^16.12.0",
    "react-dnd": "^11.1.3",
    "react-dnd-html5-backend": "^11.1.3",
    "react-dom": "^16.12.0",
    "typescript": "^3.6.4"
  },
  "devDependencies": {
    "@babel/core": "^7.10.2",
    "@babel/plugin-proposal-class-properties": "^7.10.1",
    "@babel/preset-env": "^7.10.2",
    "@babel/preset-react": "^7.10.1",
    "@types/classnames": "^2.2.10",
    "@types/jest": "^25.2.3",
    "@types/lodash": "^4.14.154",

{
  "name": "react-scheduler-calendar",
  "version": "0.3.2-5",
  "description": "React component for scheduling/planing in calendar",
  "author": "NGC-AB",
  "license": "MIT",
  "repository": "NGC-AB/react-scheduler-calendar",
  "types": "dist/index.d.ts",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "engines": {
    "node": ">=8",
    "npm": ">=5"
  },
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "test:update": "jest -u",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "generate:declaration": "tsc --declaration --allowJs --emitDeclarationOnly --outDir ./dist",
    "prepare": "yarn run build",
    "build": "yarn generate:declaration && webpack --config ./config/webpack.prod.conf.js",
    "build:watch": "yarn generate:declaration && webpack --watch --config ./config/webpack.prod.conf.js",
    "release": "np --no-publish",
    "build:demo": "webpack --config ./example/config/webpack.conf.js",
    "start:demo": "webpack-dev-server --config ./example/config/webpack.conf.js",
    "deploy:demo": "gh-pages -d ./example/dist",
    "sonar-check": "./sonar-check.sh"
  },
  "dependencies": {
    "moment": "^2.26.0",
    "react-dnd": "^11.1.3",
    "react-dnd-html5-backend": "^11.1.3",
    "rrule": "^2.6.4"
  },
  "peerDependencies": {
    "antd": "^4.0.3",
    "moment": "^2.26.0",
    "react": "^16.12.0",
    "react-dnd": "^11.1.3",
    "react-dnd-html5-backend": "^11.1.3",
    "react-dom": "^16.12.0",
    "typescript": "^3.6.4"
  },
  "devDependencies": {
    "@babel/core": "^7.10.2",
    "@babel/plugin-proposal-class-properties": "^7.10.1",
    "@babel/preset-env": "^7.10.2",
    "@babel/preset-react": "^7.10.1",
    "@babel/preset-typescript": "^7.10.1",
    "@types/classnames": "^2.2.10",
    "@types/enzyme": "^3.10.5",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/jest": "^25.2.3",
    "@types/lodash": "^4.14.154",
    "@types/react": "^16.9.35",
    "@types/react-dom": "^16.9.8",
    "@typescript-eslint/eslint-plugin": "^3.4.0",
    "@typescript-eslint/parser": "^3.4.0",
    "antd": "^4.0.3",
    "babel-jest": "^26.0.1",
    "babel-loader": "^8.1.0",
    "classnames": "^2.2.6",
    "cross-env": "^7.0.2",
    "css-loader": "^3.6.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "1.15.2",
    "enzyme-to-json": "^3.5.0",
    "eslint": "^7.3.1",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.21.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-standard": "^4.0.1",
    "file-loader": "^6.0.0",
    "gh-pages": "^3.1.0",
    "html-webpack-plugin": "^4.3.0",
    "husky": "^4.2.5",
    "jest": "^26.0.1",
    "jest-environment-jsdom": "^26.0.1",
    "node-sass": "^4.14.1",
    "np": "^6.2.3",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "rrule": "^2.6.4",
    "sass-loader": "^8.0.2",
    "sonar-scanner": "^3.1.0",
    "style-loader": "^1.2.1",
    "terser-webpack-plugin": "^3.0.2",
    "ts-loader": "^7.0.5",
    "typescript": "^3.6.4",
    "webpack": "^4.43.0",
    "webpack-bundle-analyzer": "^3.8.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^4.2.2"
  },
  "files": [
    "dist"
  ],
  "husky": {
    "hooks": {
      "pre-push": "yarn lint && yarn test"
    }
  }
}
